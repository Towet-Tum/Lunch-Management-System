{
    "builds": [
        {
            "src": "company_site/wsgi.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.12",
                "buildCommand": "pip install -r requirements.txt"
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "company_site/wsgi.py"
        }
    ]
}
